{
    "name": "avz",
    "displayName": "AvZ",
    "description": "VSCode Extension For PvZ TAS Framework : AsmVsZombies",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.27.0"
    },
    "publisher": "vector-wlc",
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/vector-wlc/AvZVSCodeExtenson"
    },
    "activationEvents": [
        "onCommand:AsmVsZombies.runScript"
    ],
    "keywords": ["avz", "pvz"],
    "icon": "image/avz.ico",
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "AsmVsZombies.runScript",
            "title": "Run Script (AvZ)"
        }],
        "menus": {
            "editor/context": [{
                "when": "editorFocus",
                "command": "AsmVsZombies.runScript",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.27.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }
}